@Article{李艳荻2017p445--453,
  Title                    = {动态特征块匹配的背景更新在运动检测的应用},
  Author                   = {李艳荻 and 徐熙平 and 陈江 and 王鹤程},
  Journal                  = {仪器仪表学报},
