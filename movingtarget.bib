  Pages                    = {18--21},

  __markedentry            = {[junhu:]},
  Abstract                 = {图像配准是多光谱相机获得精确的多光谱数据立方体的重要步骤。文中分析了运动目标对滤光片阵列型多光谱相机所获取光谱图像序列的配准结果的影响。针对此类误配准问题，文中提出一种基于运动目标检测的误配准修正方法。该方法首先通过变化检测粗提取误配准点区域，然后利用运动目标形状特征进一步提取误配准点，利用多图像序列的空间和灰度关系对误配准点进行修正。用该方法对8通道的滤光片阵列型多光谱相机的多光谱图像进行处理，实验结果表明，该方法能有效地校正多光谱成像中运动目标所造成误配准的影响，提高多光谱图像配准的准确度。处理后的图像数据更有利于图像判读、多光谱图像分类、目标识别等的后续应用。},
  Doi                      = {10.3969/j.issn.1673-629X.2016.01.004},
  Keywords                 = {多光谱图像; 配准; 运动目标; 目标检测},
  Url                      = {http://g.wanfangdata.com.cn/details/detail.do?_type=perio&id=wjfz201601005}
}

@Article{翁胜伟2017p202--205,
  Title                    = {基于Kinect的人流监测图像采集},
