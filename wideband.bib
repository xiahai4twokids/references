@Article{Zhang2016p5030--5037,
  Title                    = {A Compact Microstrip-Fed Patch Antenna With Enhanced Bandwidth and Harmonic Suppression},
  Author                   = {Zhang, J. D. and Zhu, L. and Wu, Q. S. and Liu, N. W. and Wu, W.},
  Journal                  = {Ieee Transactions on Antennas and Propagation},
  Year                     = {2016},

  Month                    = dec,
  Number                   = {12},
  Pages                    = {5030--5037},
  Volume                   = {64},

  __markedentry            = {[913:4]},
  Abstract                 = {A single-layer microstrip-fed patch antenna with capabilities of both bandwidth enhancement and harmonic suppression is proposed. For this purpose, a pair of lambda/4 microstrip-line resonators is introduced and coupled in proximity to a rectangular patch. The wideband property can be obtained by making effective use of the two resonances introduced by the radiating patch and nonradiating lambda/4 resonators. Different from other reported dual-resonance patch antennas, the proposed antenna does not require the electrically thick substrate, so it has attractive low-profile property. Thanks to the good features of lambda/4 resonators and capacitive feeding scheme, harmonic radiating modes of the patch antenna can be significantly suppressed as highly demanded in modern highly integrated communication systems. The working principle, equivalent circuit, and design procedure are extensively described. Finally, a prototype antenna operating at 4.9 GHz is designed and fabricated. The measured results show that its bandwidth is 2.7 times wider than that of the traditional insert-fed patch counterpart, and the harmful spurious radiation from other higher order radiating modes has been effectively suppressed.},
  Af                       = {Zhang, Jin-DongEOLEOLZhu, LeiEOLEOLWu, Qiong-SenEOLEOLLiu, Neng-WuEOLEOLWu, Wen},
  C1                       = {[Zhang, Jin-Dong; Wu, Wen] Nanjing Univ Sci & Technol, Sch Elect Engn & Optoelect Technol, Ministerial Key Lab, JGMT, Nanjing 210094, Jiangsu, Peoples R China.EOLEOL[Zhu, Lei; Wu, Qiong-Sen; Liu, Neng-Wu] Univ Macau, Fac Sci & Technol, Dept Elect & Comp Engn, Macau 999078, Peoples R China.},
  Da                       = {2018-11-01},
  De                       = {Bandwidth enhancement; coupled feed; harmonic suppression; patchEOLEOLantenna; quarter-wave resonator},
  Doi                      = {10.1109/TAP.2016.2618539},
  Ei                       = {1558-2221},
  Em                       = {zhangjd@mail.njust.edu.cn; leizhu@umac.mo},
  File                     = {Zhang2016p5030--5037.pdf:Zhang2016p5030--5037.pdf:PDF},
  Fu                       = {Macau Science and Technology Development Fund [FDCT-051/2014/A1];EOLEOLUniversity of Macau [MYRG2015-00010-FST, MYRG2014-00079-FST]; NationalEOLEOLNatural Science Foundation of China [61571468, 61401208]; ChinaEOLEOLPostdoctoral Science Foundation [2014M561650]},
  Fx                       = {This work was supported in part by the Macau Science and TechnologyEOLEOLDevelopment Fund under Grant FDCT-051/2014/A1, in part by the UniversityEOLEOLof Macau under Grant MYRG2015-00010-FST and Grant MYRG2014-00079-FST, inEOLEOLpart by the National Natural Science Foundation of China under GrantEOLEOL61571468 and Grant 61401208, and in part by the China PostdoctoralEOLEOLScience Foundation under Grant 2014M561650.},
  Ga                       = {EK2WP},
  J9                       = {IEEE T ANTENN PROPAG},
  Ji                       = {IEEE Trans. Antennas Propag.},
  Keywords                 = {DEFECTED GROUND STRUCTURE; PHOTONIC BANDGAP; WIRELESS COMMUNICATIONS; SHAPED PROBE; RADIATION, rank3},
  La                       = {English},
  Nr                       = {34},
  Pa                       = {445 HOES LANE, PISCATAWAY, NJ 08855-4141 USA},
  Pg                       = {8},
  Pi                       = {PISCATAWAY},
  Publisher                = {Ieee-inst Electrical Electronics Engineers Inc},
  Ranking                  = {rank4},
  Review                   = {This paper presents a new compact coupled-fed patch antenna in a single-layer substrate. By using a pair of /4 resonators, the bandwidth of the patch antenna is significantly enlarged and the harmonic radiations are effectively suppressed, while other advantages of the patch antenna, such as low cost, low profile, and easy integration, still remain. In the analysis, an equivalent circuit model is proposed to analyze and design this proposed antenna. Our investigation shows that the bandwidth of this patch can be widened by adjusting the gap between the patch and the /4 resonator. To validate the design method, a prototype antenna operating at 4.9 GHz is designed and fabricated. Its good performance is demonstrated by comparison with a traditional insert-fed patch antenna. The measured and simulated results show that the bandwidth has been enlarged by 2.7 times and the higher mode radiations have been successfully suppressed. In addition, more symmetric radiation patterns and gains have been obtained.},
  Rp                       = {Zhang, JD (reprint author), Nanjing Univ Sci & Technol, Sch Elect Engn & Optoelect Technol, Ministerial Key Lab, JGMT, Nanjing 210094, Jiangsu, Peoples R China.},
