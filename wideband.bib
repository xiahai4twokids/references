@Article{Mak2000p73--76,
  Title                    = {Broadband patch antenna with a T-shaped probe},
  Author                   = {Mak, C. L. and Lee, K. F. and Luk, K. M.},
  Journal                  = {Iee Proceedings-microwaves Antennas and Propagation},
  Year                     = {2000},

  Month                    = apr,
  Number                   = {2},
  Pages                    = {73--76},
  Volume                   = {147},

  Abstract                 = {A single-layered broadband patch antenna with a T-shaped feeding probe is studied experimentally. With a supporting foam substrate of thickness around 0.1 lambda(0), a rectangular patch antenna with bandwidth (SWR less than or equal to 2) of 40% and average gain of 7dBi is designed. Such wideband and high gain performance is confirmed by simulation using Ensemble. Furthermore, by introducing a second identical T-probe, which is fed out of phase with the original probe, the cross-polarisation of the H-plane is reduced substantially. The patch antenna with 2 T-shaped probes also maintains an impedance bandwidth (SWR less than or equal to 2) of 41% and average gain of 6.5dBi. Stable radiation patterns are observed across the passbands.},
  Doi                      = {10.1049/ip-map:20000264},
