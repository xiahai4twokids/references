@Article{Khan2016p2758--2770,
  Title                    = {Characteristic Mode Analysis of a Class of Empirical Design Techniques for Probe-Fed, U-Slot Microstrip Patch Antennas},
  Author                   = {Khan, Mahrukh and Chatterjee, Deb},
