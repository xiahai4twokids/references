  Wc                       = {Engineering, Electrical & Electronic; Telecommunications},
  Z9                       = {10}
}

@Article{Zhang2016p3310--3318,
  Title                    = {Gain-Enhanced Patch Antennas With Loading of Shorting Pins},
  Author                   = {Zhang, Xiao and Zhu, Lei},
  Journal                  = {Ieee Transactions on Antennas and Propagation},
  Year                     = {2016},

  Month                    = aug,
  Number                   = {8},
  Pages                    = {3310--3318},
  Volume                   = {64},

  __markedentry            = {[913:4]},
  Abstract                 = {A new gain-enhanced patch antenna with loading of shorting pins is proposed in this paper. Four metallic pins are symmetrically placed in the two diagonals of a square patch resonator to electrically short the patch and ground plane. These shorting pins tremendously perturb the field distribution beneath the patch due to their shunt inductive effect. As these four pins are moved outward along the two orthogonal diagonals away from the center, their influence on the field distribution over the patch is strengthened to gradually raise its dominant mode, i.e., TM010 mode, resonant frequency as the pin-to-pin spacing is enlarged. At a fixed resonant frequency, the overall area of this proposed patch antenna with four pins results to be increased. As such, its radiation directivity or gain gets to be enhanced due to the enlarged antenna area. After extensive analysis is executed, two square patch antennas with and without loaded pins are designed and fabricated. The simulated and measured results agree with each other, and they have evidently demonstrated that the radiation directivity can be enhanced up to 11.0 dBi, or about 2.9 dB increment, by virtue of the proposed approach.},
  Doi                      = {10.1109/TAP.2016.2573860},
  Ei                       = {1558-2221},
  File                     = {Zhang2016p3310--3318.pdf:Zhang2016p3310--3318.pdf:PDF},
  Ranking                  = {rank4},
  Review                   = {In this paper, a unified approach has been proposed to effectively enhance the radiation directivity and gain of a single patch antenna, in which four shorting pins are symmetrically loaded beneath the patch. Due to the shunt inductive effect of these pins, the overall electrical size of the proposed patch antenna under its TM010 mode operation becomes enlarged so as to increase its radiation area and enhance its radiation directivity and gain. After the operating principle of this antenna is described, extensive analysis has been conducted to demonstrate the enhanced radiation directivity with respect to the pin-to-pin spacing, relative permittivity of dielectric substrate, and pin radius. In sum, two square patch antennas with and without four pins are designed, fabricated, and tested. Simulated and measured results show excellent agreement, thus exhibiting that a square patch antenna can achieve a radiation directivity of up to 11.0 dBi with excessive 2.9 dB by means of the proposed approach.},
  Sn                       = {0018-926X},
  Ut                       = {WOS:000384007600005}
}

