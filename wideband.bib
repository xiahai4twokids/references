@Article{Chi2007p1457--1462,
  Title                    = {Internal compact dual-band printed loop antenna for mobile phone application},
  Author                   = {Chi, Yun-Wen and Wong, Kin-Lu},
  Journal                  = {Ieee Transactions on Antennas and Propagation},
